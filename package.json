{
    "name": "bloglist-app",
    "version": "0.0.0",
    "scripts": {
      "dev:frontend": "cd frontend && npm run dev",
      "test:frontend": "cd frontend && npm run test",
      "build:frontend": "cd frontend && npm run build",
      "lint:frontend": "cd frontend && npm run lint",
      "dev:backend": "cd backend && npm run dev",
      "test:backend": "cd backend && npm run test",
      "lint:backend": "cd backend && npm run lint",
      "start:backend": "cd backend && npm run start",
      "start:backend:test": "cd backend && npm run start:test",
      "start-prod": "cd backend && npm run start-prod",
      "copy-dist": "cp -fr frontend/dist backend/",
      "build": "npm run build:frontend && npm run copy-dist",
      "start": "npm run build && npm run start:backend",
      "lint": "npm run lint:frontend && npm run lint:backend",
      "test": "npm run test:backend && npm run test:frontend",
      "test:e2e": "cd backend && npm run test:e2e"
    }
}